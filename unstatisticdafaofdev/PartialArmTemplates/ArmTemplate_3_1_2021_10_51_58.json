{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "unstatisticdafaofdev"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"queryTimeout": "02:00:00",
								"partitionOption": "None"
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobStorageWriteSettings"
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "GenreID",
											"type": "Int16",
											"physicalType": "smallint"
										},
										"sink": {
											"name": "GenreID",
											"type": "Int16",
											"physicalType": "Int16"
										}
									},
									{
										"source": {
											"name": "GenreName",
											"type": "String",
											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "GenreName",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "sysCreatedAt",
											"type": "DateTime",
											"physicalType": "datetime2"
										},
										"sink": {
											"name": "sysCreatedAt",
											"type": "DateTime",
											"physicalType": "DateTime"
										}
									},
									{
										"source": {
											"name": "sysChangedAt",
											"type": "DateTime",
											"physicalType": "datetime2"
										},
										"sink": {
											"name": "sysChangedAt",
											"type": "DateTime",
											"physicalType": "DateTime"
										}
									},
									{
										"source": {
											"name": "sysCreatedBy",
											"type": "String",
											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "sysCreatedBy",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "sysChangedBy",
											"type": "String",
											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "sysChangedBy",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "TestDatabase",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "ds_src_csv",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/TestDatabase')]",
				"[concat(variables('factoryId'), '/datasets/ds_src_csv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/TestDatabase')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "DST"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "PersonID",
						"type": "int",
						"precision": 10
					},
					{
						"name": "FirstName",
						"type": "nvarchar"
					},
					{
						"name": "MiddleName",
						"type": "nvarchar"
					},
					{
						"name": "Surname",
						"type": "nvarchar"
					},
					{
						"name": "DateBirth",
						"type": "date"
					},
					{
						"name": "sysCreatedAT",
						"type": "datetime2",
						"scale": 3
					},
					{
						"name": "sysChangedAt",
						"type": "datetime2",
						"scale": 3
					},
					{
						"name": "sysCreatedBy",
						"type": "nvarchar"
					},
					{
						"name": "sysChangedBy",
						"type": "nvarchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "Genre"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_src_csv')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "UNStatistic_OB_LinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "DST.csv",
						"folderPath": "CountryRef",
						"container": "unstatisticresourcedata"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					}
				]
			},
			"dependsOn": []
		}
	]
}